{
  "name": "intro-storybook-react-template",
  "version": "0.2.0",
  "description": "Starter template to get up and running quickly with React and Storybook",
  "author": "Chromatic <https://chromatic.com/>",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaui/intro-storybook-react-template"
  },
  "bugs": {
    "url": "https://github.com/chromaui/intro-storybook-react-template/issues"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "styled-components": "6.1.15"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "init-msw": "msw init public/",
    "chromatic": "npx chromatic --project-token=chpt_690c2628cbd7006",
    "screenDiff": "npx playwright test",
    "screenDiff-ci": "npx playwright test -c playwright.config.ci.ts",
    "screenDiff-update": "npx playwright test --update-snapshots",
    "screenDiff-docker": "concurrently -k \"yarn run storybook\" \"docker run -w /tmp --ipc=host --entrypoint=npx --mount type=bind,src=$(pwd),dst=/tmp --rm mcr.microsoft.com/playwright:v1.50.1-noble playwright test\"",
    "screenDiff-update-docker": "concurrently -k \"yarn run storybook\" \"docker run -w /tmp --ipc=host --entrypoint=npx --mount type=bind,src=$(pwd),dst=/tmp --rm mcr.microsoft.com/playwright:v1.50.1-noble playwright test --update-snapshots\""
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@storybook/addon-a11y": "^8.5.4",
    "@storybook/addon-essentials": "^8.5.4",
    "@storybook/addon-interactions": "^8.5.4",
    "@storybook/addon-links": "^8.5.4",
    "@storybook/blocks": "^8.5.4",
    "@storybook/react": "^8.5.4",
    "@storybook/react-vite": "^8.5.4",
    "@storybook/test": "^8.5.4",
    "@storybook/test-runner": "0.21.3",
    "@types/node": "^22.13.4",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "axe-playwright": "2.1.0",
    "chromatic": "^11.25.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.11.2",
    "msw": "^2.3.0",
    "msw-storybook-addon": "^2.0.4",
    "prop-types": "^15.8.1",
    "storybook": "^8.5.4",
    "vite": "^5.2.0"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
